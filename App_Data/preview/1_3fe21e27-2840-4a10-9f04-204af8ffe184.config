                {
                  "value": "<h1 class=\"entry-title\">Family Protection</h1>\n<hr />",
                  "editor": {
                    "name": "Rich text editor",
                    "alias": "rte",
                    "view": "rte",
                    "render": null,
                    "icon": "icon-article",
                    "config": {}
