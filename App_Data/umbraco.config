    <mainColors><![CDATA[/** Colors
	#015379	= blue
	#1b1f23	= darker blue
	#454b56	= brighter blue
	#015379	= blue
	#36a0da	= darker kblue
	#82c4e8	= brighter kblue
 **************************************************************** **/

	/* theme main color [blue] */
	
	#slider h2,
	#slider .image-caption .inner p.dark,
	div.price-table.popular,
	.styleBackground {
		color:#fff;
		background-color:#015379;
	}
	/* overlay two colors */
	.overlay.color1 {
		background:rgba(41,47,56,0.8) !important;
	}
	.overlay.color2 {
		background:rgba(102,183,226,0.8) !important;
	}


	/* second color [orange] */
	a, .styleSecondColor, 
	.list-icon.color li:before,
	.item-box-desc small,
	p.dropcap:first-letter,
	ul.pagination a,
	ul.pagination a:hover,
	ul.pagination li.active .fa,
	ul.star-rated.styleSecondColor li i,
	.tp-catpion a,
	.divider.styleColor .fa {
		color:#015379;
	}
	#slider .btn.btn-default {
		color:#015379 !important;
	}

	a:hover, a:active {
		color:#36a0da;
	}


	#slider h3, .styleSecondBackground,
	.list-icon.color.circle li:before,
	.featured-box-minimal i.fa,
	.label-primary,
	div.featured-box i.fa,
	.progress-bar-primary,
	ul.pagination li.active a:hover,
	ul.pagination li.active a,
	ul.isotope-filter li.active a,
	ul.isotope-filter li.active a:hover,
	i.featured-icon,
	p.dropcap.color:first-letter,
	.blog-timeline .timeline-item .item-date span,
	ul.nav-pills li.active a,
	.modal-header {
		background-color:#015379;
	}

	.styleSecondBackground,
	.styleSecondBackground h1,
	.styleSecondBackground h2,
	.styleSecondBackground h3,
	.styleSecondBackground h4,
	.styleSecondBackground h5,
	.styleSecondBackground h6,
	.styleSecondBackground p {
		color:#fff;
	}

	/* Top Nav  */
	header#topNav nav ul.nav-main > li:hover > a, 
	header#topNav nav ul.nav-main li.active > a, 
	header#topNav nav ul.nav-main li.active > a:hover, 
	header#topNav nav ul.nav-main li.active > a:focus, 
	header#topNav nav ul.nav-main li.active i.icon-caret-down {
		color:#015379; 					/* active|hover color */
	}
	header#topNav nav.mega-menu ul.nav-main li.mega-menu-item ul.dropdown-menu,
	header#topNav nav ul.nav-main ul.dropdown-menu {
		background-color: #000000; 		/* dropdown background color */
	}
	header#topNav nav.mega-menu ul.nav-main li.mega-menu-item:hover ul.sub-menu li:hover a:hover,
	header#topNav nav ul.nav-main ul.dropdown-menu li:hover > a {
		background-color: #1b1f23; 		/* hover background color */
	}
	header#topNav nav ul.nav-main li.dropdown:hover > a:after {
		border-bottom:#015379 10px solid;/* arrow color */
	}
	header#topNav nav.mega-menu ul.nav-main li.mega-menu-item .mega-menu-sub-title {
		color: #ffffff; 				/* mega menu subtitle */
	}
	header#topNav nav.mega-menu ul.nav-main li.mega-menu-item ul.sub-menu a,
	header#topNav nav ul.nav-main ul.dropdown-menu > li > a {
		color: #afafaf; 				/* link color */
	}

	/* top Head */
	header#topHead.color {
		color:#fff;
		background-color:#015379;
	}
	header#topHead.color a {
		color:#fff;
	}

	header#page-title {
		color:#2d2f3c;
	}
	header#page-title h1,
	header#page-title h2 {
		color:#2d2f3c;
	}


	/* Buttons */
	.btn-default {
		border:#112D52 2px solid;
	}
	.btn-default:hover, .btn-default:active, .btn-default:focus {
		border:#36a0da 2px solid;
	}
	.btn-primary {
		background-color:#112D52 !important;
	}
	.btn-primary:hover, .btn-primary:active, .btn-primary:focus {
		border:rgba(255,255,255,0) 2px solid;
		background-color:#36a0da !important;
	}

	
	/* Toggle */
	div.toggle.active > label {
		color:#fff;
		background-color:#015379;
	}

	/* Panels */

	.panel-primary .panel-heading {
		background-color:#015379;
		border-bottom:#015379 1px solid;
	}
	.panel-body {
		background-color:#fff;
		-webkit-border-bottom-right-radius: 6px;
		-webkit-border-bottom-left-radius: 6px;
		-moz-border-radius-bottomright: 6px;
		-moz-border-radius-bottomleft: 6px;
		border-bottom-right-radius: 6px;
		border-bottom-left-radius: 6px;
	}

	/* Featured Box */
	div.featured-box i.fa:after {
		border-color:#015379;
	}
	div.featured-box.nobg.border-only i.fa {
		color:#015379;
		border-color:#015379;
	}
	
	/* Featured Icons */
	i.featured-icon,
	i.featured-icon:after {
		border:#015379 1px solid;
	}
	i.featured-icon.empty {
		color:#015379 !important;
	}

	/* Popover */
	.popover-title {
		color:#fff;
		background-color:#015379;
	}

	/* testimonials */
	.testimonial.color1 p,
	.testimonial.color1 cite {
		color:#fff;
		background-color:#015379;
	}
	.testimonial.color2 p,
	.testimonial.color2 cite {
		color:#fff;
		background-color:#015379;
	}

	/* OWL Carousel */
	.owl-theme .owl-controls .owl-buttons div {
		color:#015379;
	}

	/* slider */
	#slider li .noimg-slider h1{
		color:#015379 !important;
	}

	#slider li.styleBackground .noimg-slider,
	#slider li.styleBackground h1,
	#slider li.styleBackground h2,
	#slider li.styleBackground p {
		color:#fff !important;
	}
	
	/* Top Search , Top Login */
	#topHead .signin-dd form button,
	#topHead .signin-dd form button:hover,
	#topHead .signin-dd form button:active,
	header#topNav li.search form button,
	header#topNav li.search form button:hover,
	header#topNav li.search form button:active {
		background-color:#015379 !important;;
	}
	#topHead .signin-dd form button:after,
	header#topNav li.search form button:after {
		border: #015379 10px solid; /* same with .btn-primary */
		border-top: 10px solid transparent;
		border-left: 10px solid transparent;
		border-bottom: 10px solid transparent;
	}

ul.dropdown-menu
{
-webkit-box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.2);
-moz-box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.2);
box-shadow:5px 5px 5px 0px rgba(0,0,0,0.2);
}

header#topNav div.nav-main-collapse .in
{
margin:10px;
}]]></mainColors>
