    <mainColors><![CDATA[/* Buttons  */
/* George Changes Button Colours Here...............................................................................................................................  */
	.btn-default {
		border:#112D52 2px solid;
	}
	.btn-default:hover, .btn-default:active, .btn-default:focus {
		border:#36a0da 2px solid;
	}

	.btn-primary {
		background-color:#004545 !important;
	}

	.btn-primary:hover, .btn-primary:active, .btn-primary:focus {
		border:rgba(255,255,255,0) 2px solid;
		background-color:#000 !important;
	}

/* Gallery button colours..................................................................................................................................................... */

#gallery  a:hover
{
background-color:rgba(0,0,0,0.5);
}

#gallery  a:focus
{
background-color:#004545;
}

#gallery  a:active
{
background-color:#004545;
}

#gallery .nav-pills  li.active  a
{
background-color:#004545;
}

#gallery  a
{
color:#fff;
}
#gallery  a:hover
{
color:#747474;
}

/* Dropdown Colour Change..................................................................................................................................................... */
.dropdown-menu > li > a:hover, .dropdown-menu > li > a:focus
{
background-color:#747474;
}

/* Menu Shadow..................................................................................................................................................... */

ul.dropdown-menu
{
-webkit-box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.5);
-moz-box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.5);
box-shadow:5px 5px 5px 0px rgba(0,0,0,0.5);
}

header#topNav div.nav-main-collapse .in
{
margin:10px;


}]]></mainColors>
